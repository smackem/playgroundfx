<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="net.smackem.fxplayground.ExplosionController">
    <padding>
        <Insets bottom="4" left="4" right="4" top="4" />
    </padding>
    <StackPane style="-fx-background-color: black"
               AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.rightAnchor="0">
        <Canvas fx:id="canvas" width="640" height="480" />
    </StackPane>
</AnchorPane>
